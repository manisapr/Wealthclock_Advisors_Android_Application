<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#363646"
    android:clickable="true"
    tools:context="android.wealthclockadvisors.app.wealthclockadvisors.Views.fragment.DashBoard.Insurance.FirstPage_Insurance">

    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:weightSum="5"
        android:gravity="center">

        <android.support.v7.widget.CardView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/_10sdp"
            android:layout_margin="@dimen/_10sdp"
            app:cardBackgroundColor="#f7363646">

            <android.support.v4.view.ViewPager
                android:layout_width="match_parent"
                android:layout_height="175dp"
                android:id="@+id/imageslider"
                android:background="@drawable/roundcorner">

            </android.support.v4.view.ViewPager>

        </android.support.v7.widget.CardView>

        <!--<android.support.v7.widget.CardView-->

        <!--android:layout_width="350dp"-->

        <!--android:layout_height="@dimen/_150sdp"-->

        <!--android:id="@+id/listview_insurance"-->

        <!--app:cardCornerRadius="@dimen/_10sdp"-->

        <!--android:padding="@dimen/_5sdp"-->

        <!--android:layout_margin="@dimen/_10sdp"-->

        <!--app:cardBackgroundColor="#f7363646"-->

        <!--android:background="@drawable/vacation">-->

        <!--<ImageView-->

        <!--android:layout_width="340dp"-->

        <!--android:layout_height="@dimen/_70sdp"-->

        <!--android:background="@drawable/insurance1"-->

        <!--android:layout_gravity="center"/>-->

        <!--</android.support.v7.widget.CardView>-->









        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#31313f"
            android:orientation="vertical"
            android:padding="@dimen/_15sdp"
            android:weightSum="3">





            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="0sp"
                android:layout_weight="1"
                android:orientation="horizontal"
                android:weightSum="2">



                <RelativeLayout

                    android:id="@+id/terminsurance"
                    android:layout_width="0sp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_3sdp"
                    android:layout_marginRight="@dimen/_3sdp"
                    android:layout_weight="1"
                    android:background="@drawable/icon_background_dashboard">



                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/_5sdp"
                        android:layout_centerInParent="true">



                        <RelativeLayout

                            android:id="@+id/lay_buildWealth"
                            android:layout_width="@dimen/_30sdp"
                            android:layout_height="@dimen/_30sdp"
                            android:layout_centerHorizontal="true"
                            android:background="@drawable/terminsurance12">

                        </RelativeLayout>



                        <TextView

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/lay_buildWealth"
                            android:text="TERM INSURANCE"
                            android:textColor="#ffffff"
                            android:layout_margin="@dimen/_5sdp"
                            android:textSize="@dimen/_11sdp" />

                    </RelativeLayout>





                </RelativeLayout>



                <RelativeLayout
                    android:id="@+id/lifeinsurance"
                    android:layout_width="0sp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_3sdp"
                    android:layout_marginLeft="@dimen/_3sdp"
                    android:layout_weight="1"
                    android:background="@drawable/icon_background_dashboard">



                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true">



                        <RelativeLayout
                            android:id="@+id/lay_recomendation"
                            android:layout_width="@dimen/_30sdp"
                            android:layout_height="@dimen/_30sdp"
                            android:layout_centerHorizontal="true"
                            android:background="@drawable/lifeinsurance12">
                        </RelativeLayout>



                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/lay_recomendation"
                            android:layout_margin="@dimen/_5sdp"
                            android:text="LIFE INSURANCE"
                            android:textColor="#ffffff"
                            android:textSize="@dimen/_11sdp" />

                    </RelativeLayout>
                </RelativeLayout>
            </LinearLayout>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0sp"
                android:layout_weight="1"
                android:orientation="horizontal"
                android:weightSum="2">



                <RelativeLayout
                    android:id="@+id/motorinsurance"
                    android:layout_width="0sp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_3sdp"
                    android:layout_marginRight="@dimen/_3sdp"
                    android:layout_marginTop="@dimen/_3sdp"
                    android:layout_weight="1"
                    android:background="@drawable/icon_background_dashboard">



                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true">



                        <RelativeLayout
                            android:id="@+id/lay_do_it_urslf"
                            android:layout_width="@dimen/_30sdp"
                            android:layout_height="@dimen/_30sdp"
                            android:layout_centerHorizontal="true"
                            android:background="@drawable/motorinsurance122">

                        </RelativeLayout>



                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/lay_do_it_urslf"
                            android:layout_margin="@dimen/_5sdp"
                            android:text="MOTOR INSURANCE"
                            android:textColor="#ffffff"
                            android:textSize="@dimen/_11sdp" />


                    </RelativeLayout>
                </RelativeLayout>





                <RelativeLayout
                    android:id="@+id/healthinsurance"
                    android:layout_width="0sp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_3sdp"
                    android:layout_marginLeft="@dimen/_3sdp"
                    android:layout_marginTop="@dimen/_3sdp"
                    android:layout_weight="1"
                    android:background="@drawable/icon_background_dashboard">



                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true">



                        <RelativeLayout
                            android:id="@+id/lay_regular_income"
                            android:layout_width="@dimen/_30sdp"
                            android:layout_height="@dimen/_30sdp"
                            android:layout_centerHorizontal="true"
                            android:background="@drawable/healthinsurance12">

                        </RelativeLayout>



                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/lay_regular_income"
                            android:layout_margin="@dimen/_5sdp"
                            android:text="HEALTH INSURANCE"
                            android:textColor="#ffffff"
                            android:textSize="@dimen/_11sdp" />

                    </RelativeLayout>
                </RelativeLayout>
            </LinearLayout>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0sp"
                android:layout_weight="1">



                <RelativeLayout
                    android:id="@+id/generalinsurance"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_3sdp"
                    android:layout_marginTop="@dimen/_3sdp"
                    android:layout_weight="1"
                    android:background="@drawable/icon_background_dashboard">



                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true">



                        <RelativeLayout
                            android:id="@+id/lay_person"
                            android:layout_width="@dimen/_30sdp"
                            android:layout_height="@dimen/_30sdp"
                            android:layout_centerHorizontal="true"
                            android:background="@drawable/otherinsurance12">
                        </RelativeLayout>



                        <TextView
                            android:id="@+id/tv1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/lay_person"
                            android:layout_margin="@dimen/_5sdp"
                            android:text="OTHER INSURANCE"
                            android:textColor="#ffffff"
                            android:textSize="@dimen/_11sdp" />



                    </RelativeLayout>

                </RelativeLayout>
            </LinearLayout>

        </LinearLayout>



    </LinearLayout>

</FrameLayout>
