<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#2b2b37"
    android:clickable="true"
    tools:context="android.wealthclockadvisors.app.wealthclockadvisors.Views.fragment.DashBoard.FragmentContactWhatsapp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="vertical"
        android:layout_gravity="center">

        <!--<TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Lorem Ipsum is simply dummy text of the printing and typesetting industry."
            android:textColor="@color/White"
            android:layout_marginBottom="20dp"
            android:textAlignment="center"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"/>

   <ImageView
       android:layout_width="200dp"
       android:layout_height="200dp"
       android:src="@drawable/whatspp_icon" />-->

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            app:cardCornerRadius="10dp"
            app:cardBackgroundColor="#363646"
            app:cardElevation="10dp">

            <RelativeLayout
                android:id="@+id/feedbacklayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/icon"
                    android:layout_width="70dp"
                    android:layout_height="60dp"
                    android:src="@drawable/writeletter"
                    android:layout_margin="10dp"/>

                <TextView
                    android:id="@+id/feedback"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Send feedback"
                    android:layout_toRightOf="@+id/icon"
                    android:textColor="@color/White"
                    android:textSize="20dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginLeft="5dp"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Service Improvement.User experience feature etc."
                    android:layout_toRightOf="@+id/icon"
                    android:layout_below="@+id/feedback"
                    android:textColor="@color/White"
                    android:layout_marginLeft="5dp"
                    android:layout_marginBottom="10dp"/>
            </RelativeLayout>
        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            app:cardCornerRadius="10dp"
            app:cardBackgroundColor="#363646"
            app:cardElevation="10dp">

            <RelativeLayout
                android:id="@+id/ticketreg"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/icon1"
                    android:layout_width="70dp"
                    android:layout_height="60dp"
                    android:src="@drawable/ticket"
                    android:layout_margin="10dp"/>

                <TextView
                    android:id="@+id/raiseticket"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Raise Ticket"
                    android:layout_toRightOf="@+id/icon1"
                    android:textColor="@color/White"
                    android:textSize="20dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginLeft="5dp"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Service Improvement.User experience feature etc."
                    android:layout_toRightOf="@+id/icon1"
                    android:layout_below="@+id/raiseticket"
                    android:textColor="@color/White"
                    android:layout_marginLeft="5dp"
                    android:layout_marginBottom="10dp"/>
            </RelativeLayout>
        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            app:cardCornerRadius="10dp"
            app:cardBackgroundColor="#363646"
            app:cardElevation="10dp">

            <RelativeLayout
                android:id="@+id/contactButton"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/icon2"
                    android:layout_width="70dp"
                    android:layout_height="60dp"
                    android:src="@drawable/whtsap_logo"
                    android:layout_margin="10dp" />

                <TextView
                    android:id="@+id/whats"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Chat with us"
                    android:layout_toRightOf="@+id/icon2"
                    android:textColor="@color/White"
                    android:textSize="20dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginLeft="5dp"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Service Improvement.User experience feature etc."
                    android:layout_toRightOf="@+id/icon2"
                    android:layout_below="@+id/whats"
                    android:textColor="@color/White"
                    android:layout_marginLeft="5dp"
                    android:layout_marginBottom="10dp"/>
            </RelativeLayout>
        </android.support.v7.widget.CardView>


    </LinearLayout>
</FrameLayout>
