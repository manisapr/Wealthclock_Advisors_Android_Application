<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#363646"
    android:clickable="true"
    tools:context="android.wealthclockadvisors.app.wealthclockadvisors.Views.fragment.DashBoard.Insurance.Insurance_second_page">

   <LinearLayout
       android:layout_width="match_parent"
       android:layout_height="match_parent"
       android:gravity="center"
       android:orientation="vertical">
       <android.support.v7.widget.CardView
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:layout_margin="10dp"
           app:cardCornerRadius="10dp"
           app:cardBackgroundColor="#363646"
           app:cardElevation="10dp">

           <RelativeLayout
               android:id="@+id/feedbacklayout"
               android:layout_width="match_parent"
               android:layout_height="wrap_content">

               <ImageView
                   android:id="@+id/icon"
                   android:layout_width="70dp"
                   android:layout_height="60dp"
                   android:src="@drawable/writeletter"
                   android:layout_margin="10dp"/>

               <TextView
                   android:id="@+id/feedback"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="Send feedback"
                   android:layout_toRightOf="@+id/icon"
                   android:textColor="@color/White"
                   android:textSize="20dp"
                   android:layout_marginTop="10dp"
                   android:layout_marginBottom="10dp"
                   android:layout_marginLeft="5dp"/>

               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="Service Improvement.User experience feature etc."
                   android:layout_toRightOf="@+id/icon"
                   android:layout_below="@+id/feedback"
                   android:textColor="@color/White"
                   android:layout_marginLeft="5dp"
                   android:layout_marginBottom="10dp"/>
           </RelativeLayout>
       </android.support.v7.widget.CardView>
   </LinearLayout>

</FrameLayout>
